<h1>Upload New Picture</h1>
<%= form_with model: @picture do |form| %>
 <div>
  Picture url: <%= form.text_field :image_url%>
 </div>
 <div>
  Caption: <%= form.text_field :title%>
 </div>
 <div>
  Tag: <%= form.collection_check_boxes(:tag_ids, Tag.all, :id,:name) %>
 </div>
  <%= form.submit %>
<%end%>
